                  </packing>
                </child>
              </object>
              <packing>
                <property name="resize">False</property>
                <property name="shrink">False</property>
              </packing>
            </child>
            <child>
              <object class="GtkTreeView" id="jobTree">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="model">CIStore</property>
                <property name="expander_column">job</property>
                <property name="rules_hint">True</property>
                <property name="enable_tree_lines">True</property>
                <child internal-child="selection">
                  <object class="GtkTreeSelection" id="treeview-selection1"/>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="job">
                    <property name="title" translatable="yes">Job</property>
                    <child>
                      <object class="GtkCellRendererPixbuf" id="imgCell"/>
                      <attributes>
                        <attribute name="pixbuf">1</attribute>
                      </attributes>
                    </child>
                    <child>
                      <object class="GtkCellRendererText" id="jobCell">
                        <property name="wrap_mode">word</property>
                      </object>
                      <attributes>
                        <attribute name="text">0</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="resize">True</property>
                <property name="shrink">False</property>
